#include <iostream>

int main() {
    double num1, num2, sum;

    // Input two numbers
    std::cout << "Enter first number: ";
    std::cin >> num1;
    std::cout << "Enter second number: ";
    std::cin >> num2;

    // Add the numbers
    sum = num1 + num2;

    // Display the result
    std::cout << "Sum = " << sum << std::endl;

    return 0;
}
